        <% let i=1 %>
        <% quizes.forEach(quiz=>{ %>
            <div class="leaderboard_score_card_entry_quiz">
                <h6 class="student_entry_sr_quiz"><%=i  %></h6>
                <h6 class="student_entry_name_quiz"><%=quiz.title  %> </h6>
                <h6 class="student_entry_corr_quiz"><%=quiz.createdBy  %></h6>
                <h6 class="student_entry_incorr_quiz"><%=((Number(quiz.quizDB.length))*4)  %></h6>
                <h6 class="student_entry_Unattempted_quiz"><%=(Number(quiz.students_scores.length))  %></h6>
                <h6 class="student_entry_score_quiz">
                    <% if(quiz.Active==false){%>
                        Archived
                    <%} %>
                    <% if(quiz.Active==true && quiz.Started==true){%>
                        Active

                    <%}%>
                    <% if(quiz.Active==true && quiz.Started==false){%>
                        Not started

                    <%}%>
                </h6>
                
            </div>

        <% i=i+1 %>
        <% }) %>
    </div>

